{
  "name": "Mikgro",
  "version": "2.3.0",
  "description": "Aplicación para la interacción entre asistentes y presentador en eventos académicos/comerciales",
  "main": "server.js",
  "scripts": {
    "test-jenkins": "JUNIT_REPORT_PATH=./report.xml mocha --colors --reporter mocha-jenkins-reporter",
    "test": "nyc mocha -R spec",
    "start": "forever start server.js",
    "stop": "forever stop server.js"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "presentacion",
    "evento",
    "academico",
    "comercial",
    "interaccion",
    "asistentes"
  ],
  "dependencies": {
    "express": "latest",
    "forever": "^2.0.0",
    "localtunnel": "^2.0.0",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "0.4.1",
    "nyc": "^15.0.0",
    "redis": "^0.10.3",
    "should": "^4.6.0",
    "socket.io": "latest"
  },
  "repository": "https://github.com/Capriatto/Tst",
  "author": "Mikgro",
  "license": "GPL-1.0-or-later",
  "devDependencies": {
    "chai": "^4.2.0",
    "jquery": "^3.4.1",
    "jsdom": "13.2.0",
    "jsdom-global": "3.0.2",
    "mocha-jsdom": "^2.0.0",
    "moment": "^2.24.0",
    "redis-mock": "^0.43.0"
  }
}
